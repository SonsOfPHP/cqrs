{
    "name": "sonsofphp/cqrs",
    "type": "library",
    "description": "CQRS Compents",
    "keywords": ["cqrs"],
    "homepage": "https://github.com/SonsOfPHP/cqrs",
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Estes",
            "email": "joshua@sonsofphp.com"
        }
    ],
    "support": {
        "issues": "https://github.com/SonsOfPHP/sonsofphp/issues",
        "forum": "https://github.com/orgs/SonsOfPHP/discussions"
    },
    "autoload": {
        "psr-4": {
            "SonsOfPHP\\Component\\Cqrs\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "symfony/options-resolver": "^5.4|^6.0"
    },
    "require-dev": {
        "symfony/messenger": "^5.4"
    },
    "suggest": {
        "symfony/messenger": "Use message buses to handle commands asynchronously",
        "sonsofphp/event-sourcing": "Use with event sourcing"
    },
    "extra": {
        "sort-packages": true,
        "branch-alias": {
            "dev-main": "0.2.x-dev"
        }
    }
}
